<div id="header_menu" class="navbar" data-role="header" ng-controller="headerCtrl">
	<div data-role="controlgroup" data-type="horizontal">
		<a href="#" ng-class="{uibtnactivec: active1}" ng-click="clickf('feedback')" data-role="button" ><%= image_tag 'ic_feedback.png' %>
		<br/>
		Feedback</a>
		<a href="#" ng-class="{uibtnactivec: active2}" ng-click="clickf('insights')" data-role="button"><%= image_tag 'ic_insights.png' %>
		<br/>
		Insights</a>
		<a href="#"  ng-class="{uibtnactivec: active3}" ng-click="clickf('redemption')" data-role="button"><%= image_tag 'ic_redemptions.png' %>
		<br/>
		Redemptions</a>
		<a href="#"  ng-class="{uibtnactivec: active4}"  ng-click="clickf('numeric_code')" data-role="button"><%= image_tag 'ic_code.png' %>
		<br/>
		Code</a>
		<a href="#" class="logout" ng-click="showPopup()" data-role="button"><%= image_tag 'ic_logout.png' %></a>
	</div>
	<div ng-show="popup" class="popup clearfix">
		<h4>Are you sure you want to log out?</h4>
		<div class="btn floatLeft">
			<button ng-click="logout()">
				Yes
			</button>
		</div>
		<div class="btn floatLeft">
			<button ng-click="cancel()">
				No
			</button>
		</div>
	</div>
</div>
