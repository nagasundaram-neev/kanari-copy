<div id="scroller" align="center" ng-controller="redemeController" >
		<h3 class="redemptionMsg" ng-show="!redemptionList.length">Currently you do not have any redemptions</h3>
		<div class="redemptionsList" ng-show="redemptionList.length">
			<div ng-repeat="redemptions in redemptionList" class="redemptionsListRow clearfix">
				<div class="col1">{{redemptions.user.first_name}} {{redemptions.user.last_name}}</div>
				<div class="col2">Dhs {{redemptions.points}}</div>
				<div class="col3" ng-click="confirm(redemptions.id)"><span>Confirm</span></div>
			</div>
		</div>
</div>