<!DOCTYPE html>
<html ng-app="kanariTablet">
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta name="viewport" content="minimum-scale=1.0, width=device-width, maximum-scale=1.0, user-scalable=no"  />
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="default">
		<title>Kanari</title>
		<link rel="apple-touch-icon" href="<%= asset_path "logo_tabletso.png"%>" />
		<link rel="apple-touch-icon-precomposed" sizes="114x114" href="<%= asset_path "logo_tablet114.png"%>"/>
		<link rel="apple-touch-icon-precomposed" sizes="72x72" href="<%= asset_path "logo_tablet72.png" %>"/>
		<link rel="apple-touch-icon-precomposed" sizes="57x57" href="<%= asset_path "logo_tablet57.png" %>"/>
		<link href="<%= asset_path "logo_tablet748x1024.png"%> media="(device-width: 768px) and (orientation: landscape)" rel="apple-touch-startup-image"/>
		
		<!-- iPad (portrait) SPLASHSCREEN-->
		<link href="<%= asset_path "logo_tablet768x1004.png"%>" media="(device-width:: 768px) and (orientation: portrait)" rel="apple-touch-startup-image"/>
		<link rel="apple-touch-startup-image" href="<%= asset_path "logo_tablet2048x1496.png"%>" media="(device-width: 768px) and (orientation:landscape) and (-webkit-min-device-pixel-ratio: 2)" /> 
		<link rel="apple-touch-startup-image" href="<%= asset_path "logo_tablet1536x2008.png"%>" media="(device-width: 768px) and (orientation:portrait) and (-webkit-min-device-pixel-ratio: 2)" /> 
		<meta name="description" content="Kanari">
		<link href='//fonts.googleapis.com/css?family=Roboto:100' rel='stylesheet' type='text/css'>
		<link href='//fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
		<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.js"></script>
		<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.0.6/angular.min.js"></script>
		<script type="text/javascript" src='http://cdnjs.cloudflare.com/ajax/libs/jstimezonedetect/1.0.4/jstz.js'></script>
		<%= javascript_include_tag "applicationtab" %>
		<%= stylesheet_link_tag "applicationtab", :media => "all" %>
	</head>
	<body>
		<div ng-show="userLogin" class="userloggedIn" id="header_menu" class="navbar" ng-controller="headerCtrl">
			<div data-role="controlgroup" data-type="horizontal">
				<div class="headerDiv">
					<a id="feedback" href="#/feedback" class="ui_btn"  data-role="button" >
						<%= image_tag 'icon_feedback.svg' , height: '59'%>
					<br/>
					Feedback
					<br/>
					<span class="arrow-down">&nbsp;</span> </a>
				</div>
				<div class="headerDiv">
					<a id="insights" href="#/insights" class="ui_btn"  data-role="button">
						<%= image_tag 'icon_insights.svg' , height: '59' %>
					<br/>
					Insights
					<br/>
					<span class="arrow-down">&nbsp;</span> </a>
				</div>
				<div class="headerDiv">
					<a id="redemption" href="#/redemption" class="ui_btn"   data-role="button">
						<%= image_tag 'icon_redeemptions.svg' , height: '59' %>
					<br/>
					Redemptions
					<br/>
					<span class="arrow-down">&nbsp;</span> </a>
				</div>
				<div class="headerDiv">
					<a id="numeric_code" href="#/numeric_code" class="ui_btn" data-role="button">
						<%= image_tag 'icon_code.svg' , height: '59' %>
					<br/>
					Code
					<br/>
					<span class="arrow-down">&nbsp;</span> </a>
				</div>
				<div class="logoutDiv">
					<a href="javascript:void(0)" class="logOut ui_btn" ng-click="showPopup()" data-role="button">
						<%= image_tag 'icon_logout_tab.svg' , height: '29' %>
					</a>
						
				</div>
			</div>
		</div>
		<div class="clr"></div>
		<div ng-show="userLogin" class="userloggedIn marginTop" ng-show="userLogin" ng-controller="headerCtrl">
			<div ng-show="popup" class="popup clearfix">
				<h4>Are you sure you want to log out?</h4>
				<div class="btn floatLeft">
					<button ng-click="logout()">
						Yes
					</button>
				</div>
				<div class="btn floatLeft">
					<button ng-click="cancel()">
						No
					</button>
				</div>
			</div>
		</div>
		<div class="clr"></div>
		<div id="overlaySuccess">
			<%= image_tag 'ajaxloader.gif' %>
		</div>
		<div id="wrapper" class="ngView scrollable" ng-view></div>
	</body>
</html>
