<!DOCTYPE html>
<html ng-app="kanariMobile">
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="apple-mobile-web-app-status-bar-style" content="default">
		<meta name="viewport" content="minimum-scale=1.0, width=device-width, maximum-scale=1.0, user-scalable=no"  />
		<meta name="viewport" id="vp" content="initial-scale=1.0,user-scalable=no,maximum-scale=1" media="(device-height: 568px)" />
		<title>Kanari</title>
		<link rel="apple-touch-icon" href="<%= asset_path "logo_tabletso.png" %>" />
		<!-- <link rel="apple-touch-icon-precomposed" href="/assets/logo_tabletso72.png" /> -->
		<link rel="apple-touch-icon-precomposed" sizes="114x114" href="<%= asset_path "logo_tablet114.png" %>" />
		<link rel="apple-touch-icon-precomposed" sizes="72x72" href="<%= asset_path "logo_tabletso72.png" %>" />
		<link rel="apple-touch-icon-precomposed" sizes="57x57" href="<%= asset_path "logo_tablet57.png" %>" />

		<!-- <link rel="apple-touch-startup-image" href="/assets/logo_tablet320x460.png"/> -->

		<link href="<%= asset_path "logo_tablet320x460.png" %>" sizes="320x480" media="(device-height: 480px)" rel="apple-touch-startup-image">
		<!-- iPhone (Retina) LAUNCHSCREEN-->
		<link href="<%= asset_path "logo_tablet320x460.png" %>" sizes="640x920" media="(device-height: 480px) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image">
		<!-- iPhone 5+ LAUNCHSCREEN-->
		<link href="<%= asset_path "logo_tabletso320X548.png" %>" sizes="640x1136" media="(device-height: 568px)" rel="apple-touch-startup-image">

		<meta name="description" content="Kanari">
		<meta property="og:image" content="//kanari.co/launch/wp-content/uploads/2013/07/Kanari_Logo_WhiteBird_cropped.png"/>
		<script type="text/javascript" src="//maps.google.com/maps/api/js?sensor=false"></script>
		<script type="text/javascript" src="//platform.twitter.com/widgets.js"></script>
		<%= javascript_include_tag "applicationmob" %>
		<!-- <link href='//fonts.googleapis.com/css?family=Roboto:100' rel='stylesheet' type='text/css'> -->
		<!-- <link href='//fonts.googleapis.com/css?family=Roboto' rel='stylesheet' type='text/css'> -->
		<link href='//fonts.googleapis.com/css?family=Roboto:100,300' rel='stylesheet' type='text/css'>
		<link href='//fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>

		<%= stylesheet_link_tag "applicationmob", :media => "all" %>
	</head>
	<body>
		<div id="splash" data-role="page" ng-controller="commonCtrl">
			<div data-role="content"></div>
		</div>
		<div id="fb-root"></div>
	</body>
</html>
